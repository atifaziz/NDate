{
  "version": "1.0.0-beta-*",
  "title": "Date Type for .NET",
  "description": ".NET Standard library that provides a Date value type for times when you need to strictly represent a date without any time component.",
  "authors": [ "Atif Aziz" ],
  "copyright": "Copyright \u00a9 2014 Atif Aziz. All rights reserved.",

  "packOptions": {
    "projectUrl": "https://github.com/atifaziz/NDate",
    "licenseUrl": "http://www.apache.org/licenses/LICENSE-2.0",
    "owners": [ "Atif Aziz" ],
    "tags": [ "date", "time", "datetime" ],
    "files": {
      "include": "../COPYING.txt"
    }
  },

  "dependencies": {
    "NETStandard.Library": "1.6.0"
  },

  "frameworks": {
    "net20": {
      "buildOptions": {
        "define": [
          "SERIALIZABLE"
        ]
      }
    },
    "netstandard1.0": {
      "buildOptions": {
        "compile": {
          "excludeFiles": [
            "Date.Convertible.cs"
          ]
        }
      }
    },
    "netstandard1.3": {}
  }
}
